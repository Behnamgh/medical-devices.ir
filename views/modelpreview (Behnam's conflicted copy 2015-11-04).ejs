<div ng-controller="ModelController as devicefinder">
  <div ng-repeat="item in devicefinder.lists">
    <ol class="breadcrumb">
      <li><a href="#/device/ultrasound">{{item.type}}</a></li>
      <li><a href="#/{{item.type}}/{{item.brand}}">{{item.brand}}</a></li>
      <li class="active">{{item.model}}</li>
    </ol>
    <div class="row">
      <div class="col-sm-6 col-sm-offset-1">
        <center>
          <h4><a ng-href="#/compare1/{{item.type}}/{{item._id}}">(compare with other device)</a></h4>
        </center>
        <table class="table jadval">
          <thead>
            <tr>
              <th>information table</th>
              <th>information about<a ng-href="#/{{item.type}}/model/{{item._id}}"> {{item.model}} from {{item.brand}}</a></th>
            </tr>
          </thead>
          <tbody>
            <tr>
              <td class="danger">
                <center>clinicalapp</center>
              </td>
              <td class="active">
                <center>{{item.clinicalapp}}</center>
              </td>
            </tr>
            <tr>
              <td class="danger">
                <center>year</center>
              </td>
              <td class="active">
                <center>{{item.year}}</center>
              </td>
            </tr>
            <tr>
              <td class="danger">
                <center>clinicalapp</center>
